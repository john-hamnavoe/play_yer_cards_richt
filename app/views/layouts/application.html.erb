<!DOCTYPE html>
<html lang="en">
  <head>
    <%= render 'shared/metatags' %>
  </head>
  <body class="<%= body_class(params) %>">
    <div id="app">
      <div id="main" class="container-fluid">
        <nav class="d-block">
          <%= render 'shared/navbar' %>
        </nav>
        <main role="main" class="main mb-4">
          <div class="container-fluid p-3 p-md-4 p-lg-5 mt-5">
            <%= render 'shared/messages' %>
                <%= yield %>
          </div>
        </main>
      </div>
    </div>
    <footer>
    <p><%= session.id %>
    </footer>
  </body>
</html>
