<ol class="breadcrumb">
  <li class="breadcrumb-item active mr-auto">Game_players</li>
</ol>
<div class="card-columns">
  <% @game_players.each do |game_player| %>
    <div class="card card mb-4">
      <div class="card-body">
        <h5 class="card-title"><%= link_to game_player.to_s, game_player %></h5>
        <p class="card-text"><%= game_player.created_at %></p>
      </div>
    </div>
  <% end %>
</div>
